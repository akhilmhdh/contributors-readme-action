{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["airbnb-base", "airbnb-typescript/base", "standard-with-typescript", "prettier"],
    "plugins": ["prettier"],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "project": "./tsconfig.json"
    },
    "rules": {
        "prettier/prettier": ["error"],
        "import/prefer-default-export": 0,
        "@typescript-eslint/consistent-type-definitions": ["error", "type"],
        "@typescript-eslint/strict-boolean-expressions": 0
    }
}
