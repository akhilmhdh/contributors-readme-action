        "nanoid": "^3.3.1"
    },
    "devDependencies": {
        "@babel/preset-env": "^7.15.6",
        "@vercel/ncc": "^0.34.0",
        "babel-jest": "^27.2.4",
        "eslint": "^8.25.0",
